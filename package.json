{
  "name": "smartfix-service-providers",
  "version": "1.0.0",
  "description": "ExpressJS service providers platform with dependency injection",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "start:server": "node dist/app.server.js",
    "start:modular": "node dist/app.modular.js",
    "dev": "nodemon src/app.ts",
    "dev:server": "nodemon src/app.server.ts",
    "dev:modular": "nodemon src/app.modular.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:server": "tsc && echo 'Enhanced server build completed'",
    "build:modular": "tsc && echo 'Modular server build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:services": "ts-node scripts/test-services.ts",
    "test:modular": "ts-node scripts/test-modular.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "docker:build": "docker build -f Dockerfile.prod -t smartfix-api .",
    "docker:run": "docker run -p 3000:3000 smartfix-api",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp -r src/public dist/ 2>/dev/null || true"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "mongoose",
    "dependency-injection",
    "service-providers",
    "api"
  ],
  "author": "SmartFix Team",
  "license": "MIT",
  "dependencies": {
    "@decorators/di": "^3.0.0",
    "@decorators/express": "^3.0.0",
    "@decorators/server": "^1.0.0-beta.18",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartfix/service-providers-api.git"
  },
  "bugs": {
    "url": "https://github.com/smartfix/service-providers-api/issues"
  },
  "homepage": "https://github.com/smartfix/service-providers-api#readme"
}
